} */

.progress-text {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  line-height: 42rpx;
  color: #ffffff;
}

.error {
  position: absolute;
  left: 28%;
  top: calc(50% - 42rpx/2 + 119rpx);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.icon-error {
  width: 80rpx;
  height: 80rpx;
}

.text-error {
  margin-bottom: 50rpx;
  display: block;
  height: 42rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  line-height: 42rpx;
  color: #ffffff;
}

.bg-layer {
  position: absolute;
  width: 100vw;
  height: 100vh;
  top: 0;
  left: 0;
}

.green {
  z-index: 3;
  background: linear-gradient(211.84deg, #53EDD6 -1.3%, #108247 98.43%);
  transition: all 1.4516s ease;
  opacity: 1;
}

.purple {
  z-index: 2;
  background: linear-gradient(213.25deg, #B83AF3 8.29%, #5C41FB 98.32%);
  transition: all 1s ease;
  opacity: 1;
}

.blue {
  z-index: 1;
  background: linear-gradient(198.83deg, #69D7E3 14.73%, #6950FB 85.35%);
  transition: all 1.4516s ease;
  opacity: 1;
}

.load-animation {
  position: absolute;
  top: 500rpx;
  left: 180rpx;
  z-index: 4;
  height: 220px;
  width: 220px;
  background-repeat: no-repeat;
  background-image: url('/packages/ar/navigation/assets/animation/loading/css_sprites.png');
  animation: girlRun 5s steps(1, end) infinite;
  -webkit-animation: girlRun 5s steps(1, end) infinite;
}

