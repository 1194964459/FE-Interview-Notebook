.normal-z-index {
  z-index: 4;
}

.loading {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background: linear-gradient(211.84deg, #53EDD6 -1.3%, #108247 98.43%);
  animation: bkg 5s steps(1, end);
}

@keyframes bkg {
  0% {
    background: linear-gradient(211.84deg, #53EDD6 -1.3%, #108247 98.43%);
  }
  42% {
    background: linear-gradient(213.25deg, #B83AF3 8.29%, #5C41FB 98.32%);
  }
  68% {
    background: linear-gradient(198.83deg, #69D7E3 14.73%, #6950FB 85.35%);
  }
  100% {
    background: linear-gradient(198.83deg, #69D7E3 14.73%, #6950FB 85.35%);
  }
}

@-webkit-keyframes bkg {
  0% {
    background: linear-gradient(211.84deg, #53EDD6 -1.3%, #108247 98.43%);
    opacity: 1;
  }
  42% {
    background: linear-gradient(213.25deg, #B83AF3 8.29%, #5C41FB 98.32%);
  }
  68% {
    background: linear-gradient(198.83deg, #69D7E3 14.73%, #6950FB 85.35%);
  }
  100% {
    background: linear-gradient(198.83deg, #69D7E3 14.73%, #6950FB 85.35%);
  }
}

.icon-content {
  display: block;
  margin: 158rpx 148rpx 0;
  width: 453rpx;
  height: 389rpx;
}

.icon-cloud-lowVersion {
  width: 750rpx;
  height: 526rpx;
  position: absolute;
  bottom: 0;
  left: 0;
}

.resource-is-loading {
  position: absolute;
  left: 124rpx;
  top: calc(50% - 42rpx/2 + 119rpx);
  color: #ffffff;
  text-align: center;
}

.load-content {
  height: 42rpx;
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 500;
  font-size: 30rpx;
  line-height: 42rpx;
  color: #FFFFFF;
  margin: 0 auto;
}

.progress-bar {
  width: 500rpx;
  height: 28rpx;
  background: linear-gradient(313.37deg, rgba(255, 255, 255, 0.15) 28.72%, rgba(255, 255, 255, 0.15) 80%);
  backdrop-filter: blur(5.43656px);
  border-radius: 14rpx;
  margin-top: 28rpx;
  margin-bottom: 28rpx;
}

.been-loaded {
  background: linear-gradient(180deg, rgba(255, 255, 255, 0.105467) 0%, #ADD7FF 100%);
  border-radius: 14rpx;
  height: 28rpx;
  width: 30rpx;
}

